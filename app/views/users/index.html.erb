<h1>List of Users</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Private?</th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.private? ? "Yes" : "No" %></td>
      <td><%= link_to "Show details", user_path(user) %></td>
    </tr>
  <% end %>
</table>
